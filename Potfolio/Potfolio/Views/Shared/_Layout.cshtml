<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
     <div class="center">
        <header>
            <h1>Tyler Annis</h1>
        
    </div>
    <nav>
        <a href="@Url.Action("Index","Home")">Home</a>
       &nbsp;|&nbsp;
        @* &nbsp; makes a space in the text that won't break the line *@
        <!-- These are html comments and if you inspect the element these are the only comments that show up-->
        <a href="@Url.Action("Index", "About")">About</a>
        &nbsp;|&nbsp;
        <a href="@Url.Action("Images", "Work")">Work</a>
        &nbsp|&nbsp
        <a href="@Url.Action("Index", "Contact")">Contact Form</a>
    </nav>
    </header>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
    <footer>
        <p>
            Copyright 2014 Tyler Annis
        </p>
         <nav>
        <a href="@Url.Action("Index","Home")">Home</a>
       &nbsp;|&nbsp;
        @* &nbsp; makes a space in the text that won't break the line *@
        <!-- These are html comments and if you inspect the element these are the only comments that show up-->
        <a href="@Url.Action("Index", "About")">About</a>
        &nbsp;|&nbsp;
        <a href="@Url.Action("Images", "Work")">Work</a>
        &nbsp|&nbsp
        <a href="@Url.Action("Index", "Contact")">Contact Form</a>
    </nav>
    </footer>
</body>
</html>
